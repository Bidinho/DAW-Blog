<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Entity\Users" table="users">
    <unique-constraints>
      <unique-constraint name="index_users_on_email" columns="email"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="createdAt" type="datetime" column="created_at" nullable="false"/>
    <field name="updatedAt" type="datetime" column="updated_at" nullable="false"/>
    <field name="passwordDigest" type="string" column="password_digest" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="rememberDigest" type="string" column="remember_digest" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="admin" type="boolean" column="admin" nullable="true">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="activationDigest" type="string" column="activation_digest" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="activated" type="boolean" column="activated" nullable="true"/>
    <field name="activatedAt" type="datetime" column="activated_at" nullable="true"/>
    <field name="resetDigest" type="string" column="reset_digest" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="resetSentAt" type="datetime" column="reset_sent_at" nullable="true"/>
  </entity>
</doctrine-mapping>
